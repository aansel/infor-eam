<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/InspectionAspectPrompt_001" 
	xmlns="http://schemas.datastream.net/MP_entities/InspectionAspectPrompt_001" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" 
		schemaLocation="../MP_fields.xsd"/>
	<!-- ======= Beginning of InspectionAspectPrompt_001 ======= -->
	<xs:element name="InspectionAspectPrompt">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:ASPECTPROMPTID"/>
				<xs:element ref="dsf:CUSTOMFIELDELEMENTID"/>
				<xs:element ref="dsf:PROMPTUPLOADCOLUMN"/>
				<xs:element ref="dsf:PROMPTMANDATORY"/>
				<xs:element ref="dsf:PROMPTDISPLAY"/>
				<xs:element ref="dsf:PROMPTPRINT"/>
				<xs:element ref="dsf:PROMPTLENGTH" minOccurs="0"/>
				<xs:element ref="dsf:PROMPTUPPERCASE" minOccurs="0"/>
				<xs:element ref="dsf:PROMPTDEFAULT" minOccurs="0"/>
				<xs:element ref="dsf:PROMPTVALIDATE" minOccurs="0"/>
				<xs:element ref="dsf:PROMPTINSERT" minOccurs="0"/>
				<xs:element ref="dsf:PROMPTUPDATE" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
