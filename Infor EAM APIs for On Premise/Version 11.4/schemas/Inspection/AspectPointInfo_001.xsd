				<xs:element ref="dsf:DESTINATIONOBJTYPE"/>
				<xs:element ref="dsf:CONDITIONS" minOccurs="0"/>
				<xs:element ref="dsf:HISTORY" minOccurs="0"/>
				<xs:element ref="dsf:NUMBEROFCOPIES" minOccurs="0"/>
				<xs:element ref="dsf:STARTINGVALUE" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>