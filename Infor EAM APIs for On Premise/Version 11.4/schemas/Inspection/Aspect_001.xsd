<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Aspect_001" 
	xmlns="http://schemas.datastream.net/MP_entities/Aspect_001" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" 
		schemaLocation="../MP_fields.xsd"/>
	<!-- ======= Beginning of Aspect_001 ======= -->
	<xs:element name="Aspect">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:ASPECTID"/>
				<xs:element ref="dsf:CLASSID" minOccurs="0"/>
				<xs:element ref="dsf:TIMEDEPENDENCYID" minOccurs="0"/>
				<xs:element ref="dsf:RANDOMRESULT" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
				<xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
				<xs:element ref="dsf:StandardUserDefinedFields" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
