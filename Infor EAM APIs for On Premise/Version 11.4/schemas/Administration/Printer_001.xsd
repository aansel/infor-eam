<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Printer_001" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns="http://schemas.datastream.net/MP_entities/Printer_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of PRINTER_001 ======================================-->
	<xs:element name="Printer">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:PRINTERID"/>
				<xs:element ref="dsf:ORGANIZATIONID"/>
				<xs:element ref="dsf:DESTINATION" minOccurs="0"/>
				<xs:element ref="dsf:SPECIAL" minOccurs="0"/>
				<xs:element ref="dsf:TYPE" minOccurs="0"/>
				<xs:element ref="dsf:NOTUSED" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
