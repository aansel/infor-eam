<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/MetaData_001" 
	   xmlns="http://schemas.datastream.net/MP_entities/MetaData_001" 
	   xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   elementFormDefault="qualified">
	<!-- ======= Beginning of MetaData_001 ======= -->
	<xs:element name="MetaData">
		<xs:complexType>
			<xs:sequence>				
				<xs:element name="ColumnList">				
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>											
						</xs:sequence>
					</xs:complexType>			
				</xs:element>			
			</xs:sequence>				
		</xs:complexType>
	</xs:element>
</xs:schema>
