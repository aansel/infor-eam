<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/FlexSql_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/FlexSql_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of FlexSql_001 ======================================-->
	<xs:element name="FlexSql">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:FLEXSQLID"/>

                <xs:element ref="dsf:TABLENAME"          minOccurs="0"/>
                <xs:element ref="dsf:TRIGGERTYPE"        minOccurs="0"/>
                <xs:element ref="dsf:SQLSTATEMENT"       minOccurs="0"/>
                <xs:element ref="dsf:ABORTONFAILUREFLAG" minOccurs="0"/>
                <xs:element ref="dsf:REVERSERETURNFLAG"  minOccurs="0"/>
                <xs:element ref="dsf:ACTIVEFLAG"         minOccurs="0"/>
                <xs:element ref="dsf:FAILUREMESSAGE"     minOccurs="0"/>
                <xs:element ref="dsf:FLEXCOMMENT"        minOccurs="0"/>
                <xs:element ref="dsf:MUSTEXISTFLAG"      minOccurs="0"/>
                <xs:element ref="dsf:ISMOBILE"           minOccurs="0"/>
			</xs:sequence>

			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
