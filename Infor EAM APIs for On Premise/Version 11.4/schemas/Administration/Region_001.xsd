<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Region_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/Region_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of Region_001 =================-->
	<xs:element name="Region">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:REGIONID"/>
				<xs:element ref="dsf:POSTALCODE" minOccurs="0"/>
				<xs:element ref="dsf:CLASSID" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
				<xs:element ref="dsf:REFERENCEPOINT"/>
				<xs:element ref="dsf:TEMPERATUREUOM" minOccurs="0"/>
				<xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
				<xs:element ref="dsf:ACTUALTEMPSOURCE" minOccurs="0"/>
				<xs:element ref="dsf:ACTUALTEMPURL" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
