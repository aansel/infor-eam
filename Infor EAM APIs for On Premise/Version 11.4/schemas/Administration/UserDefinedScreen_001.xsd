<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/UserDefinedScreen_001" 
									xmlns="http://schemas.datastream.net/MP_entities/UserDefinedScreen_001" 
									xmlns:xs="http://www.w3.org/2001/XMLSchema" 
									xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
									xmlns:dsf="http://schemas.datastream.net/MP_fields" 
									elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<xs:element name="UserDefinedScreen">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:USERDEFINEDSCREENID"/>
				<xs:element ref="dsf:USERDEFINEDTABLENAME"/>
				<xs:element ref="dsf:ISTABSCREEN" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
				<xs:element ref="dsf:PARENTSCREENNAME" minOccurs="0"/>
				<xs:element ref="dsf:ENTITY" minOccurs="0"/>
				<xs:element ref="dsf:ALLOWCOMMENTS" minOccurs="0"/>
				<xs:element ref="dsf:ALLOWDOCUMENTS" minOccurs="0"/>
				<xs:element ref="dsf:ISGENERATED" minOccurs="0"/>
				<xs:element ref="dsf:CREATEDDATE" minOccurs="0"/>
				<xs:element ref="dsf:CREATEDBY" minOccurs="0"/>
				<xs:element ref="dsf:DATEUPDATED" minOccurs="0"/>
				<xs:element ref="dsf:UPDATEDBY" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
