<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/JobSetup_001" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns="http://schemas.datastream.net/MP_entities/JobSetup_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
<!--====================================================================================-->
<!--====================Begining of JobSetup_001===========================-->
	<xs:element name="JobSetup">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:JOBSETUPID"/>
				<xs:element ref="dsf:QUEUE"/>
				<xs:element ref="dsf:JOBTYPEID"/>
				<xs:element ref="dsf:JOBACTIVE"/>
				<xs:element ref="dsf:LASTRAN" minOccurs="0"/>
				<xs:element ref="dsf:NEXTRUN" minOccurs="0"/>
				<xs:element ref="dsf:MONTH" minOccurs="0"/>
				<xs:element ref="dsf:DAYOFMONTH" minOccurs="0"/>
				<xs:element ref="dsf:DAYOFWEEK" minOccurs="0"/>
				<xs:element ref="dsf:HOUR" minOccurs="0"/>
				<xs:element ref="dsf:MINUTE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>