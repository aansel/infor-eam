<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/ConversionOptionMapping_001" 
	xmlns="http://schemas.datastream.net/MP_entities/ConversionOptionMapping_001" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of ConversionOptionMapping ======================================-->
	<xs:element name="ConversionOptionMapping">
