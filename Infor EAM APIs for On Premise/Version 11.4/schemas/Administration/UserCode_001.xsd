<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Usercode_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/Usercode_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!-- ======= Beginning of UserCode_001 ======= -->
	<xs:element name="UserCode">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:UCODEID"/>
				<xs:element ref="dsf:SYSTEMCODE"/>
				<xs:element ref="dsf:SYSTEMDEFAULT"/>
				<xs:element ref="dsf:SYSTEMCODEICON" minOccurs="0"/>
				<xs:element ref="dsf:ICONPATH" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
				<xs:element ref="dsf:WEIGHT" minOccurs="0"/>
				<xs:element ref="dsf:COLOR" minOccurs="0"/>
				<xs:element ref="dsf:WOTYPECATEGORYID" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>