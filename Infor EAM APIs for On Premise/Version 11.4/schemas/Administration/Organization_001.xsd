	<!--====  Begining of Organization_001 ======================================-->
	<xs:element name="Organization">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:ORGANIZATIONID"/>
				<xs:element ref="dsf:CURRENCYCODE"/>
				<xs:element ref="dsf:LOCALE" minOccurs="0"/>
				<xs:element ref="dsf:COMMON" minOccurs="0"/>
				<xs:element ref="dsf:DUNSNUMBER" minOccurs="0"/>
