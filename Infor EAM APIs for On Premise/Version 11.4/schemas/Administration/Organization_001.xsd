				<xs:element ref="dsf:OFFSETTIME" minOccurs="0"/>
				<xs:element ref="dsf:DEPRECIATIONTYPE" minOccurs="0"/>
				<xs:element ref="dsf:ACCOUNTSEGMENTVALUE" minOccurs="0"/>
				<xs:element ref="dsf:ACCOUNTINGENTITY" minOccurs="0"/>
				<xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
                <xs:element ref="dsf:STREAMPLUSPROJECT" minOccurs="0"/>
                <xs:element ref="dsf:CALENDARGROUPID" minOccurs="0"/>                
				<xs:element ref="UserDefinedFields" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="UserDefinedFields">
		<xs:complexType>
			<xs:sequence>
