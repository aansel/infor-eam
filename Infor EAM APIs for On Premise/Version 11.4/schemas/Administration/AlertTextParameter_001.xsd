				<xs:element ref="dsf:PARAMETERFIELDID" minOccurs="0"/>
				<xs:element ref="dsf:TEXTPARAMETERVALUE" minOccurs="0"/>
				<xs:element ref="dsf:INCLUDERECIPIENT" minOccurs="0"/>
				<xs:element ref="dsf:REPORTPARAMETERNUMBER" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>