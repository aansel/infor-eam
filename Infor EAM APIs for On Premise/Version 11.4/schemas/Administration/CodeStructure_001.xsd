<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/CodeStructure_001" xmlns="http://schemas.datastream.net/MP_entities/CodeStructure_001" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dsf="http://schemas.datastream.net/MP_fields" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of CodeStructure_001 ======================================-->
	<xs:element name="CodeStructure">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:CODESTRUCTURECODE" minOccurs="0"/>
				<xs:element ref="dsf:DESCRIPTION"/>
				<xs:element ref="dsf:CODESTRUCTUREENTITY"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL1"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL2" minOccurs="0"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL3" minOccurs="0"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL4" minOccurs="0"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL5" minOccurs="0"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL6" minOccurs="0"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL7" minOccurs="0"/>
				<xs:element ref="dsf:CODESTRUCTURELEVEL8" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
