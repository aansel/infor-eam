<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/UserLanguage_001" 
	xmlns="http://schemas.datastream.net/MP_entities/UserLanguage_001" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	elementFormDefault="qualified">
	<!-- =========== Imported namespaces ============ -->
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!-- ======= Beginning of UserLanguage_001 ======= -->
	<xs:element name="UserLanguage">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:LANGUAGEID"/>
				<xs:element ref="dsf:RSTATUS" minOccurs="0"/>
				<xs:element ref="dsf:PROCESSSTART" minOccurs="0"/>
				<xs:element ref="dsf:PROCESSEND" minOccurs="0"/>
				<xs:element ref="dsf:LASTCREATED" minOccurs="0"/>				
				<xs:element ref="dsf:INSTALLED" minOccurs="0"/>
				<xs:element ref="dsf:AVAILABLE" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
				<xs:element ref="dsf:INPROCESS" minOccurs="0"/>
				<xs:element ref="dsf:LANGERRORMESSAGE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>