<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/KitSession_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/KitSession_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<xs:element name="KitSession">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:DBSESSIONID"/>
				<xs:element ref="dsf:PARTID"/>
				<xs:element ref="dsf:STOREID"/>
				<xs:element ref="dsf:BINID"/>
				<xs:element ref="dsf:KITCOUNT"/>
				<xs:element ref="dsf:LOTDESCRIPTION"/>
				<xs:element ref="dsf:MAXAVAILABLEKITS" minOccurs="0"/>
				<xs:element ref="dsf:LOTEXPIREDATE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
