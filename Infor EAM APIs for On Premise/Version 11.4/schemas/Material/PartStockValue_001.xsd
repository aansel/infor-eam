<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/PartStockValue_001" 
			xmlns:dsf="http://schemas.datastream.net/MP_fields" 
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xmlns:xs="http://www.w3.org/2001/XMLSchema" 
			xmlns="http://schemas.datastream.net/MP_entities/PartStockValue_001" 
			elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of PartStockValue_001 ==========================================-->
	<xs:element name="PartStockValue">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:PARTSTOCKVALUEPK"/>
				<xs:element ref="dsf:PARTID"/>
				<xs:element ref="dsf:STOREID"/>
				<xs:element ref="dsf:PARTCONDITIONTEMPLATECONDITIONCODE" minOccurs="0"/>
				<xs:element ref="dsf:STOCKQTY" minOccurs="0"/>
				<xs:element ref="dsf:PRICE" minOccurs="0"/>
				<xs:element ref="dsf:PRICETYPE" minOccurs="0"/>
				<xs:element ref="dsf:PURCHASEORDERLINEID" minOccurs="0"/>
				<xs:element ref="dsf:DATEREQUESTED" minOccurs="0"/>
				<xs:element ref="dsf:CURRENCYID" minOccurs="0"/>
				<xs:element ref="dsf:INVOICEID" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
