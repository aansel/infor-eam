<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/PartPrices_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/PartPrices_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of PartPrices_001 ======================================-->
	<xs:element name="PartPrices">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:PARTID"/>
				<xs:element ref="dsf:ORGANIZATIONID"/>
				<xs:element ref="dsf:AVERAGEPRICE" minOccurs="0"/>
				<xs:element ref="dsf:LASTPRICE" minOccurs="0"/>
				<xs:element ref="dsf:STANDARDPRICE" minOccurs="0"/>
				<xs:element ref="dsf:BASEPRICE" minOccurs="0"/>
				<xs:element ref="dsf:COREVALUE" minOccurs="0"/>
				<xs:element ref="dsf:CREDITBALANCE" minOccurs="0"/>
				<xs:element ref="dsf:PRICETYPE" minOccurs="0"/>
				<xs:element ref="dsf:CURRENCYID" minOccurs="0"/>
				<xs:element ref="dsf:PARTCONDITIONTEMPLATECONDITIONCODE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
