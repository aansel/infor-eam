<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/EOQDefault_001"
	xmlns="http://schemas.datastream.net/MP_entities/EOQDefault_001"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dsf="http://schemas.datastream.net/MP_fields"
	xmlns:dse-rpf="http://schemas.datastream.net/MP_entities/ReportFilter_001"
	elementFormDefault="qualified">
	<!-- =========== Imported namespaces ============ -->
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<xs:import namespace="http://schemas.datastream.net/MP_entities/ReportFilter_001" schemaLocation="../Base/ReportFilter_001.xsd"/>
	<!-- ======= Beginning of EOQDefault_001 ======= -->
	<xs:element name="EOQDefault">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:STOREID" minOccurs="0"/>
				<xs:element ref="dsf:ORGANIZATIONID" minOccurs="0"/>
				<xs:element ref="dsf:FROMDATE"/>
				<xs:element ref="dsf:TODATE"/>
				<xs:element ref="dse-rpf:ReportFilter" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
