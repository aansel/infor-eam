			<xs:sequence>
				<xs:element ref="dsf:PARTID"/>
				<xs:element ref="dsf:MANUFACTURERID"/>
				<xs:element ref="dsf:MANUFACTURERPARTCODE" minOccurs="0"/>
				<xs:element ref="dsf:MANUFACTURERDRAW" minOccurs="0"/>
				<xs:element ref="dsf:ISPRIMARY" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
				<xs:element ref="dsf:PARTCONDITIONTEMPLATECONDITIONCODE" minOccurs="0"/>
				<xs:element ref="dsf:DOCUMOTOBOOKID" minOccurs="0"/>
				<xs:element ref="dsf:DOCUMOTOPARTID" minOccurs="0"/>
				<xs:element ref="dsf:PARTMANUFACTURERID" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
			<xs:attribute name="manufacturerpartcode_new" type="dsf:CODE50_Type"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
