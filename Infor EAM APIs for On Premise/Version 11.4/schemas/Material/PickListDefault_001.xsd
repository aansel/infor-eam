<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/PickListDefault_001" 
	xmlns="http://schemas.datastream.net/MP_entities/PickListDefault_001" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of PickListDefault_001 ======================================-->
	<xs:element name="PickListDefault">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:STOREID" minOccurs="0"/>
				<xs:element ref="dsf:ORIGINID" minOccurs="0"/>
				<xs:element ref="dsf:DATEREQUIRED" minOccurs="0"/>
				<xs:element ref="dsf:STATUS" minOccurs="0"/>
				<xs:element ref="dsf:PICKLISTAPPLIMIT" minOccurs="0"/>				
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
