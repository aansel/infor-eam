<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/InventoryTransaction_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/InventoryTransaction_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of InventoryTransaction_001 ======================================-->
	<xs:element name="InventoryTransaction">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:TRANSACTIONID"/>
				<xs:element ref="dsf:STOREID"/>
				<xs:element ref="dsf:TRANSACTIONSTATUS" minOccurs="0"/>
				<xs:element ref="dsf:PARTID" minOccurs="0"/>
				<xs:element ref="dsf:PARTCLASSID" minOccurs="0"/>
				<xs:element ref="dsf:FROMBIN" minOccurs="0"/>
				<xs:element ref="dsf:TOBIN" minOccurs="0"/>
				<xs:element ref="dsf:STOCKCLASSID" minOccurs="0"/>
				<xs:element ref="dsf:ABCCODE" minOccurs="0"/>
				<xs:element ref="dsf:STOCKDATE" minOccurs="0"/>
				<xs:element ref="dsf:INCLUDECHILDSTORES" minOccurs="0"/>
				<xs:element ref="dsf:INCLUDECONSIGNMENTITEM" minOccurs="0"/>
				<xs:element ref="dsf:INCLUDEOUTOFSERVICEITEM" minOccurs="0"/>
				<xs:element ref="dsf:ASSIGNEDTO" minOccurs="0"/>
				<xs:element ref="dsf:StandardUserDefinedFields" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
