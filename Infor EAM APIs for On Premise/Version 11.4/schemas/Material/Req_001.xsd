    <xs:complexType>
      <xs:sequence>
	<xs:element ref="dsf:REQUISITIONID" minOccurs="1"/>
	<xs:element ref="dsf:STATUS" minOccurs="1"/>
	<xs:element ref="dsf:STOREID" minOccurs="1"/>
