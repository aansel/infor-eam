				<xs:element ref="dsf:JOBPLAN" minOccurs="0"/>
				<xs:element ref="dsf:MULTIPLETRADES" minOccurs="0"/>
				<xs:element ref="dsf:PLANNINGLEVEL" minOccurs="0"/>
