<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/AdditionalCharge_001"
	xmlns:dsf="http://schemas.datastream.net/MP_fields"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://schemas.datastream.net/MP_entities/AdditionalCharge_001"
		elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields"
		schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of AdditionalCharge_001 ======================================-->
	<xs:element name="AdditionalCharge">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:ADDITIONALCHARGEID"/>
				<xs:element ref="dsf:TRADECODE"/>
				<xs:element ref="dsf:ADDITIONALCHARGEDESC"/>
				<xs:element ref="dsf:ADDITIONALCHARGEQTY"/>
				<xs:element ref="dsf:UNITPRICE"/>
				<xs:element ref="dsf:CURRENCYCODE"/>
				<xs:element ref="dsf:JOBSEQUENCE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
