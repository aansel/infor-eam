<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/ProductionRequest_001"
    xmlns="http://schemas.datastream.net/MP_entities/ProductionRequest_001"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:dsf="http://schemas.datastream.net/MP_fields"
    elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd" />
    <!--===============================================================================-->
    <xs:element name="ProductionRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:PRODUCTIONREQUESTID"/>
                <xs:element ref="dsf:WORKORDERID" minOccurs="0"/>
                <xs:element ref="dsf:PRODUCTIONREQUESTSTATUS"/>
                <xs:element ref="dsf:PRODUCTIONREQUESTRSTATUS"/>
                <xs:element ref="dsf:PRODUCTIONPRIORITYID"/>
                <xs:element ref="dsf:PRODUCTIONREQUESTSTARTDATE"/>
                <xs:element ref="dsf:PRODUCTIONREQUESTENDDATE"/>
                <xs:element ref="dsf:PRODUCTIONORDER" minOccurs="0"/>
                <xs:element ref="dsf:PRODUCTIONSTARTDATE" minOccurs="0"/>
                <xs:element ref="dsf:PRODUCTIONENDDATE" minOccurs="0"/>
                <xs:element ref="dsf:ENTERPRISELOCATION" minOccurs="0"/>
                <xs:element ref="dsf:ACCOUNTINGENTITY" minOccurs="0"/>
                <xs:element ref="dsf:CLASSID" minOccurs="0"/>
                <xs:element ref="dsf:CREATEDDATE" minOccurs="0"/>
                <xs:element ref="dsf:CREATEDBY" minOccurs="0"/>
                <xs:element ref="dsf:REVISIONREASON" minOccurs="0"/>
                <xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="recordid" type="xs:long" />
        </xs:complexType>
    </xs:element>
</xs:schema>
