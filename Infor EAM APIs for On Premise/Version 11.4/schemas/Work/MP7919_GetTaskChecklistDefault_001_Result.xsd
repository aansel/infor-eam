<xs:schema targetNamespace="http://schemas.datastream.net/MP_results/MP7919_001"
    xmlns="http://schemas.datastream.net/MP_results/MP7919_001"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:dsf="http://schemas.datastream.net/MP_fields"
    elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
    <!--===============================================================================-->
    <xs:element name="MP7919_GetTaskChecklistDefault_001_Result">
    <xs:complexType>
            <xs:sequence>
                <xs:element ref="ResultData"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ResultData">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:OUTOFSERVICE"/>
                <xs:element ref="dsf:TYPE"/>
                <xs:element ref="dsf:REQUIREDTOCLOSEDOC"/>
                <xs:element ref="dsf:EQUIPMENTLEVEL"/>
                <xs:element ref="dsf:REPEATINGOCCURRENCES"/>
                <xs:element ref="dsf:ISOLATIONMETHOD" minOccurs="0"/>
                <xs:element ref="dsf:RSTATUS" minOccurs="0"/>
                <xs:element ref="dsf:REUSABLE" minOccurs="0"/>
                <xs:element ref="dsf:JOBPLANID" minOccurs="0"/>
                <xs:element ref="dsf:JOBSEQUENCE" minOccurs="0"/>
                <xs:element ref="dsf:CASEMANAGEMENTCHECKLIST" minOccurs="0"/>
                <xs:element ref="dsf:NONCONFORMITYCHECKLIST" minOccurs="0"/>
				<xs:element ref="dsf:ENABLEENHANCEDPLANNING" minOccurs="0"/>
				<xs:element ref="dsf:PERFORMBYRESPONSIBILITY" minOccurs="0"/>
				<xs:element ref="dsf:PERFORMBY2RESPONSIBILITY" minOccurs="0"/> 
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>