<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/ServiceRequest_001" xmlns="http://schemas.datastream.net/MP_entities/ServiceRequest_001" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dsf="http://schemas.datastream.net/MP_fields" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<!--====  Begining of ServiceRequest_001 ======================================-->
	<xs:element name="ServiceRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:SERVICEREQUESTID"/>
				<xs:element ref="dsf:CLASSID" minOccurs="0"/>
				<xs:element ref="dsf:AREA" minOccurs="0"/>
				<xs:element ref="dsf:ASSIGNEDTO" minOccurs="0"/>
				<xs:element ref="dsf:CALLEREMAIL" minOccurs="0"/>
				<xs:element ref="dsf:CALLERPHONE" minOccurs="0"/>
				<xs:element ref="dsf:CALLERNAME" minOccurs="0"/>
				<xs:element ref="dsf:CONTACTINFO" minOccurs="0"/>
				<xs:element ref="dsf:COSTESTIMATE" minOccurs="0"/>
				<xs:element ref="dsf:CREATIONDATE" minOccurs="0"/>
				<xs:element ref="dsf:CURRENCYID" minOccurs="0"/>
				<xs:element ref="dsf:CUSTOMERID" minOccurs="0"/>
				<xs:element ref="dsf:DUPLICATEPARENT" minOccurs="0"/>
				<xs:element ref="dsf:REPLICATEPARENT" minOccurs="0"/>
				<xs:element ref="dsf:ENTEREDBY" minOccurs="0"/>
				<xs:element ref="dsf:ENTEREDBYCALLERID" minOccurs="0"/>
				<xs:element ref="dsf:INFOTYPE" minOccurs="0"/>
				<xs:element ref="dsf:LEVEL1" minOccurs="0"/>
				<xs:element ref="dsf:LEVEL2" minOccurs="0"/>
				<xs:element ref="dsf:LEVEL3" minOccurs="0"/>
				<xs:element ref="dsf:SYSTEMID" minOccurs="0"/>
				<xs:element ref="dsf:PRIORITY" minOccurs="0"/>
				<xs:element ref="dsf:RESPONSETIME" minOccurs="0"/>
				<xs:element ref="dsf:CUSTOMERREFERENCE" minOccurs="0"/>
				<xs:element ref="dsf:STATUS"/>
				<xs:element ref="dsf:TYPE"/>
				<xs:element ref="dsf:SERVICECODEID" minOccurs="0"/>
				<xs:element ref="dsf:REPORTED" minOccurs="0"/>
				<xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
				<xs:element ref="dsf:StandardUserDefinedFields" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
			<xs:attribute name="has_department_security" type="dsf:FLAG_Type"/>
			<xs:attribute name="has_wo" type="dsf:FLAG_Type"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
