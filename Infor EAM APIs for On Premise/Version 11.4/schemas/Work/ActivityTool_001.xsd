<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Activitytool_001" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns="http://schemas.datastream.net/MP_entities/Activitytool_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!-- ======= Beginning of ActivityTool_001 ======= -->
	<xs:element name="ActivityTool">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:ACTIVITYTOOLID"/>
