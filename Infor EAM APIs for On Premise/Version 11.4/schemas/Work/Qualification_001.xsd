<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Qualification_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/Qualification_001" elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
        <!--=====================================================================-->
        <!--====  Begining of Qualification_001 ======================================-->
        <xs:element name="Qualification">
            <xs:complexType>
                <xs:sequence>
                    <xs:element ref="dsf:QUALIFICATIONID"/>
                    <xs:element ref="dsf:CLASSID" minOccurs="0"/>
                    <xs:element ref="dsf:ACTIVEFLAG" minOccurs="0"/>
                    <xs:element ref="dsf:TRAININGRECORD" minOccurs="0"/>
                    <xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
                </xs:sequence>
            <xs:attribute name="recordid" type="xs:long"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
