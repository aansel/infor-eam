	</xs:sequence>			<xs:attribute name="recordid" type="xs:long"/>		</xs:complexType>	</xs:element>	<xs:element name="MultiplePMWorkPackageData">		<xs:complexType>			<xs:sequence>				<xs:element ref="MultiplePMWorkPackageEquipmentData" maxOccurs="unbounded"/>				<xs:element ref="dsf:PMWORKPACKAGEID"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="MultiplePMWorkPackageEquipmentData">		<xs:complexType>			<xs:sequence>				<xs:element ref="dsf:PMWORKPACKAGEEQUIPID"/>			</xs:sequence>			<xs:attribute name="recordid" type="xs:long"/>		</xs:complexType>	</xs:element></xs:schema>