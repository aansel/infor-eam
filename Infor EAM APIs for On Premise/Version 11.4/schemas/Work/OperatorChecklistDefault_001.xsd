<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/OperatorChecklistDefault_001"
    xmlns="http://schemas.datastream.net/MP_entities/OperatorChecklistDefault_001"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:dsf="http://schemas.datastream.net/MP_fields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
    <!--===============================================================================-->
    <xs:element name="OperatorChecklistDefault">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:CREATEDBY" minOccurs="0"/>
                <xs:element ref="dsf:STARTDATE" minOccurs="0"/>
                <xs:element ref="dsf:ORGANIZATIONID" minOccurs="0"/>
                <xs:element ref="dsf:TASKLISTID" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="overwritetask" type="dsf:FLAG_Type" default = "false"/>
        </xs:complexType>
    </xs:element>
</xs:schema>