<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/WorkOrderSchedule_001"
           xmlns="http://schemas.datastream.net/MP_entities/WorkOrderSchedule_001"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:of="http://www.openapplications.org/oagis_fields"
           xmlns:dsf="http://schemas.datastream.net/MP_fields"
           elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<xs:element name="WorkOrderSchedule">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:WOSCHEDULEID"/>
				<xs:element ref="dsf:SCHEDULEDDATE"/>
				<xs:element ref="dsf:SCHEDULEDHOURS" minOccurs="0"/>
				<xs:element ref="dsf:STARTTIME" minOccurs="0"/>
				<xs:element ref="dsf:ENDTIME" minOccurs="0"/>
				<xs:element ref="dsf:TRADEID" minOccurs="0"/>
				<xs:element ref="dsf:DEPARTMENTID" minOccurs="0"/>
				<xs:element ref="dsf:SHIFTID" minOccurs="0"/>
				<xs:element ref="dsf:EMPLOYEE" minOccurs="0"/>
				<xs:element ref="dsf:MAINTENANCEEQUIPMENTID" minOccurs="0"/>
				<xs:element ref="dsf:ENTEREDBY" minOccurs="0"/>
				<xs:element ref="dsf:FROZEN" minOccurs="0"/>
				<xs:element ref="dsf:WOSCHEDULECOMMENT" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
