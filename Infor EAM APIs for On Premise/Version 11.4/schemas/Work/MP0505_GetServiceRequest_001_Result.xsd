			</xs:sequence>
			<xs:attribute name="can_reopen" type="dsf:FLAG_Type" default="false"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ResultData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dse-srq:ServiceRequest"/>
