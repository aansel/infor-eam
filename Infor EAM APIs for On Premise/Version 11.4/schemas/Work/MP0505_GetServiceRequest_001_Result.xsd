<xs:schema targetNamespace="http://schemas.datastream.net/MP_results/MP0505_001" xmlns="http://schemas.datastream.net/MP_results/MP0505_001" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:dse-srq="http://schemas.datastream.net/MP_entities/ServiceRequest_001" elementFormDefault="qualified">
