<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/CustomerContractAdjustmentDefault_001"
        xmlns="http://schemas.datastream.net/MP_entities/CustomerContractAdjustmentDefault_001"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:dsf="http://schemas.datastream.net/MP_fields"
        elementFormDefault="qualified">
        <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
        <!--===============================================================================-->
        <xs:element name="CustomerContractAdjustmentDefault">
            <xs:complexType>
                <xs:sequence>
                	<xs:element ref="dsf:USERCONTRACTID" />
                	<xs:element ref="dsf:ADJUSTMENTDATE" />
                	<xs:element ref="dsf:ADJUSTMENTSTATUS" />
                	<xs:element ref="dsf:ADJUSTMENTTYPE" />
                    <xs:element ref="dsf:CURRENCYCODE" minOccurs="0"/>
                    <xs:element ref="dsf:TAXID" minOccurs="0"/>
                    <xs:element ref="dsf:TAXPERCENTAGE" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>
        </xs:element>
</xs:schema>