<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/FleetException_001"
	xmlns="http://schemas.datastream.net/MP_entities/FleetException_001"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dsf="http://schemas.datastream.net/MP_fields"
	elementFormDefault="qualified">
	<!-- =========== Imported namespaces ============ -->
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!-- ======= Beginning of FleetException_001 ======= -->
	<xs:element name="FleetException">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:FLEETEXCEPTIONID"/>
				<xs:element ref="dsf:STANDARDWO" minOccurs="0"/>
				<xs:element ref="dsf:BILLINGAMOUNT" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
