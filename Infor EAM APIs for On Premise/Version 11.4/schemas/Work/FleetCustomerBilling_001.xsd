<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/FleetCustomerBilling_001"
	xmlns="http://schemas.datastream.net/MP_entities/FleetCustomerBilling_001"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dsf="http://schemas.datastream.net/MP_fields"
	elementFormDefault="qualified">
	<!-- =========== Imported namespaces ============ -->
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--====================  Begining of FleetCustomerBilling ====================-->
	<xs:element name="FleetCustomerBilling">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:FLEETCUSTOMERBILLINGID" minOccurs="0"/>
				<xs:element ref="dsf:FLEETCUSTOMERID"/>
				<xs:element ref="dsf:FLEETBILLINGID" minOccurs="0"/>
				<xs:element ref="dsf:FLEETMARKUPID" minOccurs="0"/>
				<xs:element ref="dsf:CATEGORYID" minOccurs="0"/>
				<xs:element ref="dsf:OBJTYPE" />
				<xs:element ref="dsf:EQUIPMENTCLASSID" minOccurs="0"/>
				<xs:element ref="dsf:MAINTENANCELABOR" minOccurs="0"/>
				<xs:element ref="dsf:MAINTENANCECONTRACTLABOR" minOccurs="0"/>
				<xs:element ref="dsf:MAINTENANCEPARTS" minOccurs="0"/>
				<xs:element ref="dsf:NONMAINTENANCELABOR" minOccurs="0"/>
				<xs:element ref="dsf:NONMAINTENANCECONTRACTLABOR" minOccurs="0"/>
				<xs:element ref="dsf:NONMAINTENANCEPARTS" minOccurs="0"/>
				<xs:element ref="dsf:FUEL" minOccurs="0"/>
				<xs:element ref="dsf:MILEAGE" minOccurs="0"/>
				<xs:element ref="dsf:FLEETCUSTOMERUSAGE" minOccurs="0"/>
				<xs:element ref="dsf:INSURANCE" minOccurs="0"/>
				<xs:element ref="dsf:EXCEPTION" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>