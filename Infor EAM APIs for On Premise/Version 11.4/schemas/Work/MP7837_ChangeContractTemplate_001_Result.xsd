<xs:schema targetNamespace="http://schemas.datastream.net/MP_results/MP7837_001"
        xmlns="http://schemas.datastream.net/MP_results/MP7837_001"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:dsf="http://schemas.datastream.net/MP_fields"
        elementFormDefault="qualified">
        <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
        <!--===============================================================================-->
        <xs:element name="MP7837_ChangeContractTemplate_001_Result">
        <xs:complexType>
                <xs:sequence>
                    <xs:element ref="ResultData"/>
                </xs:sequence>
            </xs:complexType>
        </xs:element>
        <xs:element name="ResultData">
            <xs:complexType>
                <xs:sequence>
                    <xs:element ref="dsf:CLASSID" minOccurs="0"/>
                    <xs:element ref="dsf:COMMON" minOccurs="0"/>
                    <xs:element ref="dsf:ROUNDINGHOURS" minOccurs="0"/>
                    <xs:element ref="dsf:ROUNDINGDAYS" minOccurs="0"/>
                    <xs:element ref="dsf:STARTTIMEHOURS" minOccurs="0"/>
        			<xs:element ref="dsf:STARTTIMEMINUTES" minOccurs="0"/>
        			<xs:element ref="dsf:ENDTIMEHOURS" minOccurs="0"/>
        			<xs:element ref="dsf:ENDTIMEMINUTES" minOccurs="0"/>
        			<xs:element ref="dsf:INVOICEEVERYPERIODS" minOccurs="0"/>
        			<xs:element ref="dsf:PERIODUOM" minOccurs="0"/>
        			<xs:element ref="dsf:NEXTINVOICEDATE" minOccurs="0"/>
        			<xs:element ref="dsf:StandardUserDefinedFields" minOccurs="0"/>
                    <xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0" />
                </xs:sequence>
            </xs:complexType>
        </xs:element>
</xs:schema>