<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/ClosingCode_001" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns="http://schemas.datastream.net/MP_entities/ClosingCode_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
<!--====================================================================================-->
<!--====================Begining of ClosingCode_001===========================-->
	<xs:element name="ClosingCode">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:CLOSINGCODEID"/>
				<xs:element ref="dsf:GENERAL" minOccurs="0"/>
				<xs:element ref="dsf:PARTFAILURE" minOccurs="0"/>
				<xs:element ref="dsf:CLOSINGCODEGROUP" minOccurs="0"/>
				<xs:element ref="dsf:ENABLEWORKORDERS" minOccurs="0"/>
				<xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>





