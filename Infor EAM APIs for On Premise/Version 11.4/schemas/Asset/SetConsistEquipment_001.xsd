<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/SetConsistEquipment_001"
    xmlns="http://schemas.datastream.net/MP_entities/SetConsistEquipment_001"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:dsf="http://schemas.datastream.net/MP_fields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
    <!--===============================================================================-->
    <xs:element name="SetConsistEquipment">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:SETCONSISTEQUIPMENTID"/>
                <xs:element ref="dsf:SETPOSITION" minOccurs="0"/>
                <xs:element ref="dsf:CONSISTPOSITION" minOccurs="0"/>
                <xs:element ref="dsf:SETID" minOccurs="0"/>
                <xs:element ref="dsf:DEPARTMENTCODE" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="recordid" type="xs:long"/>
            <xs:attribute name="has_department_security" type="dsf:FLAG_Type"/>
        </xs:complexType>
    </xs:element>
</xs:schema>