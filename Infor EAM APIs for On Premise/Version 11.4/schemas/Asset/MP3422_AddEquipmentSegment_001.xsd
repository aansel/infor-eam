<xs:schema targetNamespace="http://schemas.datastream.net/MP_functions/MP3422_001"
	xmlns="http://schemas.datastream.net/MP_functions/MP3422_001"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields"
