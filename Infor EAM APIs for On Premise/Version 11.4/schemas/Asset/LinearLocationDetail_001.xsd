				<xs:element ref="dsf:LINEARUOMID" minOccurs="0"/>
				<xs:element ref="dsf:LINEARDIRECTIONID" minOccurs="0"/>
				<xs:element ref="dsf:LINEARREFERENCEEVENT" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
