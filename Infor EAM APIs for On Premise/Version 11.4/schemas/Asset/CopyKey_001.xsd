<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/CopyKey_001"
		xmlns="http://schemas.datastream.net/MP_entities/CopyKey_001"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:dsf="http://schemas.datastream.net/MP_fields"
		elementFormDefault="qualified">
		<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
		<!--===============================================================================-->
		<xs:element name="CopyKey">
			<xs:complexType>
				<xs:sequence>
					<xs:element ref="dsf:OLDKEYID"/>
					<xs:element ref="dsf:NEWKEYID"/>
					<xs:element ref="dsf:COPYLOCKS" minOccurs="0"/>
					<xs:element ref="dsf:COPYCOMMENTS" minOccurs="0"/>
					<xs:element ref="dsf:COPYDOCUMENTS" minOccurs="0"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
</xs:schema>