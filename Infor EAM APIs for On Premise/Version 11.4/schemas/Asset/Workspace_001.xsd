<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Workspace_001"
        xmlns="http://schemas.datastream.net/MP_entities/Workspace_001"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:dsf="http://schemas.datastream.net/MP_fields"
        elementFormDefault="qualified">
        <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
        <!--===============================================================================-->
        <xs:element name="Workspace">
            <xs:complexType>
                <xs:sequence>
                    <xs:element ref="dsf:WORKSPACEID"/>
                    <xs:element ref="dsf:DEPARTMENTID" minOccurs="0"/>
                    <xs:element ref="dsf:STATUS" minOccurs="0"/>
                    <xs:element ref="dsf:TYPE" minOccurs="0"/>
                    <xs:element ref="dsf:CLASSID" minOccurs="0"/>
                    <xs:element ref="dsf:LOCATION" minOccurs="0"/>
                    <xs:element ref="dsf:FLOOR" minOccurs="0"/>
                    <xs:element ref="dsf:SEATINGDESIGNATION" minOccurs="0"/>
                    <xs:element ref="dsf:FLOORAREA" minOccurs="0"/>
                    <xs:element ref="dsf:FLOORAREAUOM" minOccurs="0"/>
                    <xs:element ref="dsf:OUTOFSERVICE" minOccurs="0"/>
                    <xs:element ref="dsf:MULTIPLEEMPLOYEES" minOccurs="0"/>
                    <xs:element ref="dsf:EMPLOYEEID" minOccurs="0"/>
                    <xs:element ref="dsf:PARENTEQUIPMENTID" minOccurs="0"/>
                    <xs:element ref="dsf:StandardUserDefinedFields" minOccurs="0"/>
                    <xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0" />
                </xs:sequence>
                <xs:attribute name="recordid" type="xs:long"/>
            </xs:complexType>
        </xs:element>
</xs:schema>