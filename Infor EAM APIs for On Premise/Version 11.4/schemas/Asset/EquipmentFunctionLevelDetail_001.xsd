<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/EquipmentFunctionLevelDetail_001"
        xmlns="http://schemas.datastream.net/MP_entities/EquipmentFunctionLevelDetail_001"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:dsf="http://schemas.datastream.net/MP_fields"
        elementFormDefault="qualified">
        <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
        <!--===============================================================================-->
        <xs:element name="EquipmentFunctionLevelDetail">
            <xs:complexType>
                <xs:sequence>
