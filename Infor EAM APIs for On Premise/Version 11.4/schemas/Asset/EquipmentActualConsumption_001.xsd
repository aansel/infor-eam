				<xs:element ref="dsf:ACTUALCONSUMPTIONRATE" minOccurs="0"/>
				<xs:element ref="dsf:ACTUALCONSUMPTIONRATEUOM" minOccurs="0"/>
				<xs:element ref="dsf:LOADFACTOR" minOccurs="0"/>
			</xs:sequence>
		<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
