<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/InvoiceVoucherCostDefault_001" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns="http://schemas.datastream.net/MP_entities/InvoiceVoucherCostDefault_001"	elementFormDefault="qualified">
