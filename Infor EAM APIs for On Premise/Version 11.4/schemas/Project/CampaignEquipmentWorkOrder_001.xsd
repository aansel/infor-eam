<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/CampaignEquipmentWorkOrder_001"
    xmlns:dsf="http://schemas.datastream.net/MP_fields"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns="http://schemas.datastream.net/MP_entities/CampaignEquipmentWorkOrder_001"
    elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
    <!-- ======= Beginning of CampaignEquipmentWorkOrder_001 ======= -->
    <xs:element name="CampaignEquipmentWorkOrder">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:CAMPAIGNEVENTID"/>
                <!-- WO Description -->
                <xs:element ref="dsf:DESCRIPTION" minOccurs="0"/>
                <!-- WO Status -->
                <xs:element ref="dsf:STATUS"/>
                <!-- WO Header Equipment for the Survey or Multi Equipment Workorder -->
                <xs:element ref="dsf:EQUIPMENTID" minOccurs="0"/>
                <xs:element ref="dsf:MULTIEQUIP" minOccurs="0"/>
                <xs:element ref="dsf:INCLUDECAMPAIGNCOMMENTS" minOccurs="0"/>
                <xs:element ref="dsf:INCLUDECAMPAIGNDOCUMENTS" minOccurs="0"/>
                <xs:element ref="dsf:SURVEY" minOccurs="0"/>
                <xs:element ref="dsf:TARGETDATE"/>
                <xs:element ref="dsf:SCHEDEND"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>

