<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/Shutdown_001" 
	xmlns="http://schemas.datastream.net/MP_entities/Shutdown_001" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dsf="http://schemas.datastream.net/MP_fields" 
	elementFormDefault="qualified">
	<!-- =========== Imported namespaces ============ -->
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!-- ======= Beginning of Shutdown_001 ======= -->
	<xs:element name="Shutdown">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:SHUTDOWNID"/>
				<xs:element ref="dsf:CLASSID" minOccurs="0"/>
				<xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>