<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/CustomerSurvey_001" xmlns:dsf="http://schemas.datastream.net/MP_fields" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.datastream.net/MP_entities/CustomerSurvey_001" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
	<!--===============================================================================-->
	<xs:element name="CustomerSurvey">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="dsf:CUSTOMERSURVEYID"/>
				<xs:element ref="dsf:STATUS"/>
				<xs:element ref="dsf:CLASSID" minOccurs="0"/>
				<xs:element ref="dsf:SURVEYTITLE" minOccurs="0"/>
				<xs:element ref="dsf:OPENINGMESSAGE"/>
				<xs:element ref="dsf:CLOSINGMESSAGE"/>
				<xs:element ref="dsf:REDIRECTURL" minOccurs="0"/>
				<xs:element ref="dsf:REDIRECTTIMER" minOccurs="0"/>
				<xs:element ref="dsf:EFFECTIVE"/>
				<xs:element ref="dsf:DISCONTINUE"/>
				<xs:element ref="dsf:URL" minOccurs="0"/>
				<xs:element ref="dsf:EMAIL" minOccurs="0"/>
				<xs:element ref="dsf:StandardUserDefinedFields" minOccurs="0"/>
				<xs:element ref="dsf:USERDEFINEDAREA" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="recordid" type="xs:long"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
