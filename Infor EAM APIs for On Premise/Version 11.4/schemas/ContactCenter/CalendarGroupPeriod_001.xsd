<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/CalendarGroupPeriod_001" 
    xmlns="http://schemas.datastream.net/MP_entities/CalendarGroupPeriod_001" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:dsf="http://schemas.datastream.net/MP_fields" 
    elementFormDefault="qualified">
    <xs:import namespace="http://schemas.datastream.net/MP_fields" schemaLocation="../MP_fields.xsd"/>
    <!--===============================================================================-->
    <!--====  Begining of CalendarGroupPeriod_001 ======================================-->
    <xs:element name="CalendarGroupPeriod">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="dsf:CALPERIODID"/>
                <xs:element ref="dsf:STARTDATE"/>
                <xs:element ref="dsf:ENDDATE"/>
                <xs:element ref="dsf:SUNTIME" minOccurs="0"/>
                <xs:element ref="dsf:MONTIME" minOccurs="0"/>
                <xs:element ref="dsf:TUESTIME" minOccurs="0"/>
                <xs:element ref="dsf:WEDTIME" minOccurs="0"/>
                <xs:element ref="dsf:THURSTIME" minOccurs="0"/>
                <xs:element ref="dsf:FRITIME" minOccurs="0"/>
                <xs:element ref="dsf:SATTIME" minOccurs="0"/>
                <xs:element ref="dsf:INTERNALOFFICEHOURS" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="recordid" type="xs:long"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
