<xs:schema targetNamespace="http://schemas.datastream.net/MP_entities/IPRequestHeader_001" xmlns="http://schemas.datastream.net/MP_entities/IPRequestHeader_001" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ipf="http://schemas.datastream.net/IP_fields" elementFormDefault="qualified">
	<xs:import namespace="http://schemas.datastream.net/IP_fields" schemaLocation="../IP_fields.xsd"/>
	<!--====  Begining of IPRequestHeader_001 ======================================-->
	<xs:element name="BLOCKCONTROL">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ipf:ACTIONTYPE"/>
				<xs:element ref="ipf:MP2USERID"/>
				<xs:element ref="ipf:EMAIL"/>
				<xs:element ref="ipf:LANGUAGEID"/>
				<xs:element ref="ipf:MP2PRODUCTID"/>
				<xs:element ref="ipf:PRODUCT" minOccurs="0"/>
				<xs:element ref="ipf:TIMESTAMP" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
